package ask.me.assignment;

import java.util.ArrayList;

public class RequestResponse {

	private Company fields;
	private int sucess;
	private int count;
	private ArrayList<Company> records;
	private String help;

	public void setFields(Company fields) {
		this.fields = fields;
	}

	public Company getFields() {
		return fields;
	}

	public void setSucess(int sucess) {
		this.sucess = sucess;
	}

	public int getSucess() {
		return sucess;
	}

	public void setCount(int count) {
		this.count = count;
	}

	public int getCount() {
		return count;
	}

	public void setRecords(ArrayList<Company> records) {
		this.records = records;
	}

	public ArrayList<Company> getRecords() {
		return records;
	}

	public void setHelp(String help) {
		this.help = help;
	}

	public String getHelp() {
		return help;
	}
}
